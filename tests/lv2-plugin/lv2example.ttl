@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#>.

<dplug:destructorizer#ui> 
    a ui:X11UI;
    ui:binary <libdistort.so> .

<dplug:destructorizer>
	a lv2:Plugin ,
		lv2:AmplifierPlugin ;
	doap:name "Dplug" ;
	doap:license <http://opensource.org/licenses/isc> ;
    ui:ui <dplug:destructorizer#ui>;
        

	lv2:project <https://github.com/AuburnSounds/Dplug> ;
    lv2:requiredFeature urid:map ;
    lv2:port [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 0 ;
		lv2:symbol "input" ;
		lv2:name "Input" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 1 ;
		lv2:symbol "drive" ;
		lv2:name "Dive" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "output" ;
		lv2:name "Output" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "enabled" ;
		lv2:name "Enabled" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 4 ;
		lv2:symbol "inleft" ;
		lv2:name "InL"
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 5 ;
		lv2:symbol "inright" ;
		lv2:name "InR"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 6 ;
		lv2:symbol "outleft" ;
		lv2:name "OutL"
	]  , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 7 ;
		lv2:symbol "outright" ;
		lv2:name "OutR"
	] .